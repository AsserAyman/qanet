{
  "expo": {
    "name": "Qanet",
    "slug": "qanet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-base.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asserayman.qanet",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/moon-image.png",
          "imageWidth": 180,
          "dark": {
            "backgroundColor": "#000000",
            "image": "./assets/images/moon-image.png"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "To calculate the start time of the Last Third of the night based on your location."
        }
      ],
      "@react-native-community/datetimepicker",
      "expo-font",
      "expo-localization",
      "expo-web-browser",
      "expo-notifications",
      [
        "@sentry/react-native/expo",
        {
          "url": "https://sentry.io/",
          "project": "react-native",
          "organization": "asser-ayman"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b0061622-12f7-4d16-8268-dfe7f560da5b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b0061622-12f7-4d16-8268-dfe7f560da5b"
    },
    "android": {
      "package": "com.asserayman.qanet"
    }
  }
}
