{
  "expo": {
    "name": "Qanet",
    "slug": "qanet",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-base.jpeg",
    "splash": {
      "image": "./assets/images/splash-moon.jpeg",
      "resizeMode": "cover",
      "backgroundColor": "#1a1a1a"
    },
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.asserayman.qanet",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-community/datetimepicker",
      "expo-font",
      "expo-localization",
      "expo-web-browser",
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b0061622-12f7-4d16-8268-dfe7f560da5b"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b0061622-12f7-4d16-8268-dfe7f560da5b"
    },
    "android": {}
  }
}
